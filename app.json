{
  "expo": {
    "name": "Locate!",
    "slug": "Locate",
    "version": "0.13.0",
    "orientation": "landscape",
    "icon": "./assets/splash.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFD874"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow Locate! to access your photos.",
        "NSPhotoLibraryAddUsageDescription": "Allow $(PRODUCT_NAME) to save photos"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#FFD874"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCvvr5eEoQWVxoymDBPrxsuGACMykMp6eg"
        }
      },
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ],
      "package": "com.wkalmar.Locate",
      "versionCode": 13
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Locate! to access your photos."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "ecded475-7a73-4e43-8b27-720f573b06cf"
      }
    }
  },
  "react-native-google-mobile-ads": {
    "android_app_id": ""
  }
}
